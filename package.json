{
  "name": "kushoai-assignment",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run lint && tsc --project tsconfig.prod.json",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "DOTENV_CONFIG_PATH=./config/.env.development ts-node ./src/main",
    "dev:watch": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "start": "DOTENV_CONFIG_PATH=./config/.env.production node -r dotenv/config -r module-alias/register ./dist/main.js",
    "type-check": "tsc -b --noEmit"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "jet-env": "^1.1.6",
    "jet-logger": "^2.2.3",
    "jet-paths": "^2.0.1",
    "jet-validators": "^2.2.6",
    "jsonfile": "^6.2.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@swc/core": "^1.15.11",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/jsonfile": "^6.1.4",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.10",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.23.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
